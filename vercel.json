{
  "cleanUrls": true,
  "framework": null,
  "rewrites": [
    { "source": "/(.*).mp3", "destination": "/$1.mp3" }
  ],
  "headers": [
    {
      "source": "/(.*).mp3",
      "headers": [
        { "key": "Content-Type", "value": "audio/mpeg" },
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}